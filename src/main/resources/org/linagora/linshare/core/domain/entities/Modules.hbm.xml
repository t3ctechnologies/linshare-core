<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="org.linagora.linshare.core.domain.entities.Modules" table="modules">  
    <id name="id" column="id">  
     <generator class="guid"></generator>  
    </id>  
            
    <property name="name" column="name"></property>  
    <property name="description" column="description"></property> 
    <property name="createDate" column="createDate"></property>
    <property name="modifiedDate" column="modifiedDate"></property>  
    <property name="statusId" column="statusId"></property> 
          
  </class>  
</hibernate-mapping>